/*================ COMPONENTS ================*/

.overlay-content {
    position: fixed;
    overflow-x: scroll;
    top: 0;
    left: 0;
    width: 60rem;
    max-height: 80vh;
    height: 60rem;
    z-index: 500;
    opacity: 0;
    pointer-events: none;
    visibility: hidden;
    transition: opacity 0.5s cubic-bezier(0.72, 0, 0.28, 1) 0.3s, visibility 0.5s cubic-bezier(0.72, 0, 0.28, 1) 0.3s;
    background: white;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.overlay-content .inner {
    opacity: 0;
    transition: opacity 0.5s cubic-bezier(0.72, 0, 0.28, 1) 0s, visibility 0.5s cubic-bezier(0.72, 0, 0.28, 1) 0s;
}

