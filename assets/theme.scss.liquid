}

.overlay-bg {
    background: rgba(0, 0, 0, 0.46);
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    pointer-events: none;
    opacity: 0;
    visibility: hidden;
    transition: opacity 0.5s ease, visibility 0.5s ease;
    z-index: 490;
}

.active-overlay .overlay-content {
    opacity: 1;
    pointer-events: auto;
    visibility: visible;
    transition: opacity 0.5s cubic-bezier(0.72, 0, 0.28, 1) 0s, visibility 0.5s cubic-bezier(0.72, 0, 0.28, 1) 0s;
}

.active-overlay .overlay-content .inner {
    opacity: 1;
    transition: opacity 1s cubic-bezier(0.72, 0, 0.28, 1) 0.3s, visibility 1s cubic-bezier(0.72, 0, 0.28, 1) 0.3s;
}

.active-overlay .overlay-bg {
    pointer-events: auto;
